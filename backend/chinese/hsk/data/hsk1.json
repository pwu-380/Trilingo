  "grammar": [
    {
      "pattern": "Subject + 是 + Noun",
      "english": "A is B (Identity)",
      "example": "我是学生。"
    },
    {
      "pattern": "Subject + 很 + Adjective",
      "english": "Subject is [adjective] (Description)",
      "example": "他很高。"
    },
    {
      "pattern": "Subject + 不 + Verb/Adj",
      "english": "Negation with 不",
      "example": "我不喝咖啡。"
    },
    {
      "pattern": "Subject + Verb + 了",
      "english": "Completed action / Change of state",
      "example": "我吃了。 / 下雨了。"
    },
    {
      "pattern": "Subject + 在 + Place + Verb",
      "english": "Action at a location",
      "example": "我在学校学习。"
    },
    {
      "pattern": "…… 吗？",
      "english": "Yes/no question particle",
      "example": "你好吗？"
    },
    {
      "pattern": "Subject + 有 + Object",
      "english": "To have / there is (Existence)",
      "example": "我有一个哥哥。"
    },
    {
      "pattern": "Noun + 的 + Noun",
      "english": "Possessive / Descriptive particle",
      "example": "我的书。"
    },
    {
      "pattern": "Subject + Time + Verb",
      "english": "Time placement (When something happens)",
      "example": "我明天去北京。"
    },
    {
      "pattern": "Subject + 会/能/想/要 + Verb",
      "english": "Modal verbs (Ability/Desire)",
      "example": "我会说汉语。 / 我想去商店。"
    },
    {
      "pattern": "太 + Adjective + 了",
      "english": "Too / Extremely",
      "example": "太热了！"
    },
    {
      "pattern": "Verb + 一下",
      "english": "Do something briefly / soften tone",
      "example": "你看一下。"
    }
  ],
